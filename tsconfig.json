{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "NodeNext",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../lib"
    },
    "include": ["./src/*.ts"],
    "exclude": ["!./node_modules/**/*.ts"]
}
